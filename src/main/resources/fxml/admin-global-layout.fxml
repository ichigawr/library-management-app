<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<StackPane xmlns:fx="http://javafx.com/fxml/1" fx:id="stackPaneContainer" maxHeight="720.0" maxWidth="1280.0"
           minHeight="720.0" minWidth="1280.0" xmlns="http://javafx.com/javafx/23"
           fx:controller="app.libmgmt.view.admin.AdminGlobalController">
  <children>
    <Pane fx:id="backgroundPane" prefHeight="720.0" prefWidth="1280.0">
      <children>
        <HBox fx:id="globalFormContainer">

          <children>
            <fx:include source="admin-navigation.fxml"/>

            <VBox fx:id="contentContainer">
              <fx:include source="admin-header.fxml"/>
              <Pane fx:id="pagingPane" prefHeight="670.0" prefWidth="1132.0">
                <children>
                  <fx:include source="admin-dashboard.fxml"/>
                </children>
              </Pane>

            </VBox>
          </children>

        </HBox>
      </children>
    </Pane>
  </children>
</StackPane>
